<h2>Users cadastrados</h2>

<table border="1">
    <tr> <!-- table row -->
        <th>Id</th>
        <th>Nickname</th>
        <th>Nome</th>
    </tr>
    {{#each usuarios}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.nickname}}</td>
        <td>{{this.nome}}</td>
    </tr>
    {{/each}}
</table>