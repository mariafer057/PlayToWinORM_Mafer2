<h1>Conquistas Cadastrados</h1>

<a href="/jogos/{{jogo.id}}/novaConquista">Cadastrar uma conquista</a>

<table border="1">
  <!-- table row -->
  <tr>
    <!-- table head -->
    <th>Nome</th>
    <th>Descrição</th>
  </tr>
  {{#each conquistas}}
    <tr>
      <td>{{this.nome}}</td>
      <td>{{this.descricao}}</td>
    </tr>
  {{/each}}
</table>