<h2>Jogos Cadastrados</h2>


<a href="/jogos/novo">Cadastrar jogos</a>
<table border="1">
    <tr> <!-- table row -->
        <th>Id</th>
        <th>Titulo</th>
        <th>Descrição</th>
        <th>Preço</th>

        
    </tr>
    {{#each jogos}}
    <tr>
        <td>{{this.id}}</td>
        <td>{{this.titulo}}</td>
        <td>{{this.descricao}}</td>
        <td>{{this.preco}}</td>

        <td>
            <a href="/jogos/{{this.id}}/update">Atualizar</a>
            <form action="/jogos/{{this.id}}/delete" method="post">
            <input type="submit" value="Excluir"/>
            </form>
        </td>
        
    </tr>
    {{/each}}
</table>